{
  "name": "is-github-repo",
  "description": "Checks if string is a git repository",
  "version": "0.0.2",
  "main": "index.js",
  "repository": "git@github.com:bukinoshita/is-git-repository.git",
  "author": "Bu Kinoshita <bukinoshita@gmail.com>",
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "keywords": [
    "git",
    "github",
    "repository",
    "repo",
    "url",
    "https",
    "http"
  ],
  "scripts": {
    "test": "nyc ava",
    "lint": "xo"
  },
  "devDependencies": {
    "ava": "^2.1.0",
    "eslint-config-prettier": "^5.0.0",
    "husky": "^2.4.1",
    "lint-staged": "^8.2.1",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "xo": "^0.24.0"
  },
  "xo": {
    "extends": [
      "prettier"
    ]
  },
  "lint-staged": {
    "*.js": [
      "prettier --semi false --single-quote --write",
      "yarn lint",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
